        <div class="card sect_f-card--colour">
          <div class="card-body">
            <div class="sect_f-text-img--container">
              <span class="sect_f-img-container">
                <i class="fas fa-check"></i>
              </span>
              <div class="sect_f-text-container--7">
                <h3 class="sect_f-text--7">Student Discounts</h3>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
